<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/saved_routes_navigation"
    app:startDestination="@id/savedFragment">

    <fragment
        android:id="@+id/savedFragment"
        android:name="com.example.hikingapp.ui.profile.saved.SavedFragment"
        android:label="fragment_saved"
        tools:layout="@layout/fragment_saved" >
        <action
            android:id="@+id/action_savedFragment_to_savedRouteFragment"
            app:destination="@id/savedRouteFragment" />
    </fragment>
    <fragment
        android:id="@+id/savedRouteFragment"
        android:name="com.example.hikingapp.ui.profile.saved.SavedRouteFragment"
        android:label="fragment_saved_route"
        tools:layout="@layout/fragment_saved_route" >
        <action
            android:id="@+id/action_savedRouteFragment_to_savedFragment"
            app:destination="@id/savedFragment" />
    </fragment>
</navigation>